{
  "swagger": "2.0",
  "info": {
    "version": "1.1",
    "title": "Foster API's"
  },
  "basePath": "/foster",
  "schemes": [
    "http",
    "http"
  ],
  "consumes": [
    "application/json",
    "application/x-www-form-urlencoded",
    "multipart/form-data"
  ],
  "produces": [
    "application/json"
  ],
  "tags": [
    {
      "name": "user",
      "description": "User Apis"
    }
  ],
  "paths": {
    "/user/register": {
      "post": {
        "tags": [
          "user"
        ],
        "description": "Send chat message sms or pns",
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "parameters": [
          {
            "name": "email",
            "in": "formData",
            "description": "email id",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "in": "formData",
            "description": "password",
            "required": true,
            "type": "string"
          },
          {
            "name": "name",
            "in": "formData",
            "description": "name",
            "required": true,
            "type": "string"
          },
          {
            "name": "registration-id",
            "in": "header",
            "description": "Registration id",
            "required": false,
            "type": "string"
          },
          {
            "name": "platform",
            "in": "header",
            "description": "Platform 1 - iOS, 2 - Android",
            "required": false,
            "type": "integer"
          },
          {
            "name": "image",
            "in": "formData",
            "description": "profile pic",
            "required": false,
            "type": "file"
          },
          {
            "name": "address",
            "in": "formData",
            "description": "address",
            "required": true,
            "type": "string"
          },
          {
            "name": "latitude",
            "in": "formData",
            "description": "latitude",
            "required": true,
            "type": "number"
          },
          {
            "name": "longitude",
            "in": "formData",
            "description": "longitude",
            "required": true,
            "type": "number"
          }
        ]
      }
    },
    "/user/login": {
      "post": {
        "tags": [
          "user"
        ],
        "description": "Login with email / password",
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "parameters": [
          {
            "name": "email",
            "in": "formData",
            "description": "email id",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "in": "formData",
            "description": "Password",
            "required": true,
            "type": "string"
          },
          {
            "name": "registration-id",
            "in": "header",
            "description": "Registration id",
            "required": false,
            "type": "string"
          },
          {
            "name": "platform",
            "in": "header",
            "description": "Platform 1 - iOS, 2 - Android",
            "required": false,
            "type": "integer"
          }
        ]
      }
    },
    "/user/verifyAccount": {
      "post": {
        "tags": [
          "user"
        ],
        "description": "verifyAccount-Confirmation",
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "parameters": [
          {
            "name": "token",
            "in": "formData",
            "description": "token",
            "required": true,
            "type": "string"
          }
        ]
      }
    }
  },
  "/swagger": {
    "x-swagger-pipe": "swagger_raw"
  },
  "definitions": {
    "HelloWorldResponse": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      }
    },
    "ErrorResponse": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      }
    }
  }
}